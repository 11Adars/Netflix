{
  "rewrites": [
    { "source": "/assets/:path*", "destination": "/assets/:path*" },
    { "source": "/:asset(favicon.ico|logo192.png|logo512.png|manifest.json|robots.txt)", 
      "destination": "/:asset" },
    { "source": "/data/:path*", "destination": "/data/:path*" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}